<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label fx:id="idLabelNombreTexto" onMouseClicked="#onNombreClicked" text="Nombre">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Label>
      <Label fx:id="idLabelNombre" maxWidth="100.0" minWidth="100.0" text="Label" GridPane.columnIndex="1" />
      <Label fx:id="idLabelEdadTexto" onMouseClicked="#onEdadClicked" text="Edad" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Label>
      <Label fx:id="idLabelEdad" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>

</GridPane>
